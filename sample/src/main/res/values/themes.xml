<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="Theme.Swec" parent="Theme.MaterialComponents.DayNight.NoActionBar">
    <!-- Primary brand color. -->
    <item name="colorPrimary">@color/color_333333</item>
    <item name="colorPrimaryVariant">@color/color_454a4d</item>
    <item name="colorOnPrimary">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/color_dddddd</item>
    <item name="colorSecondaryVariant">@color/color_dddddd</item>
    <item name="colorOnSecondary">@color/black</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    <!-- Customize your theme here. -->
    <item name="windowActionBar">false</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="windowNoTitle">true</item>
  </style>
</resources>